{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "./src/main/main.ts",
  "scripts": {
    "dev": "vite serve ./src/render",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:render": "vite serve ./src/render",
    "start:electron": "electronmon -r ts-node/register/transpile-only .",
    "start": "concurrently \"npm run start:render \" \"electronmon -r ts-node/register/transpile-only .\""
  },
  "dependencies": {
    "antd": "^5.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "styled-components": "6.0.0-rc.6",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "electron": "^25.2.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}